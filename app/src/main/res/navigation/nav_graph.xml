<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/vehiclePlaceholderListFragment">

    <fragment
        android:id="@+id/vehiclePlaceholderListFragment"
        android:name="com.example.searchvehicleapp.ui.vehicle.pageradapter.VehiclePlaceholderListFragment"
        android:label="VehiclePlaceholderListFragment" >
        <action
            android:id="@+id/action_vehiclePlaceholderListFragment_to_vehicleDetailFragment"
            app:destination="@id/vehicleDetailFragment" />
    </fragment>

    <fragment
        android:id="@+id/vehicleDetailFragment"
        android:name="com.example.searchvehicleapp.ui.vehicle.detailfragments.VehicleDetailFragment"
        android:label="fragment_vehicle_detail"
        tools:layout="@layout/fragment_vehicle_detail" >
        <action
            android:id="@+id/action_vehicleDetailFragment_to_vehiclePlaceholderListFragment"
            app:destination="@id/vehiclePlaceholderListFragment"
            app:popUpTo="@id/vehiclePlaceholderListFragment"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>